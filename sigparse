'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
' 남은 티켓이 얼마나 되나 알아보기 위한 것...
sql = "SELECT count(num) FROM SIGGRAPH_RGST where  REGI<>'GroupMember'and ElectronicTheatre like '%16 Dec 2010: 19:00~21:00%'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'GroupMember'and ElectronicTheatre like '%17 Dec 2010: 19:00~21:00%'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'GroupMember'and ElectronicTheatre like '%18 Dec 2010: 16:15~18:15%'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'GroupMember'and ElectronicTheatre like '%18 Dec 2010: 19:00~21:00%'"  '첫째날..
'##################### 추가 티켓 좌석이 남아있는지 확인 ####################3
sql = "SELECT sum(Convert(int,AddElectronicTheatre15)),sum(Convert(int,AddElectronicTheatre16)),sum(Convert(int,AddElectronicTheatre17)),sum(Convert(int,AddElectronicTheatre18)) FROM SIGGRAPH_RGST where REGI<>'Group'"
Elect15_rev    = (trim(info(0))+ Con1DaySoo)  ' 전체 개수 - 첫째날 여분 수..
sqlstr = "select count(*) from "&tb&"  where REGI<>'Group'"&event_query2
sql = "select convert(smalldatetime,left(app_date,11)), cnt = count(convert(smalldatetime,left(app_date,11))) from "&tb&"  where REGI<>'GroupMember'"&event_query2&"   group by convert(smalldatetime,left(app_date,11)) order by convert(smalldatetime,left(app_date,11)) desc"

