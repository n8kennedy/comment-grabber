Response.Write "<td width='11' align='right'><img src='/_cfman/images/num_right.gif' width='6' height='7'></td>"
'response.write "<td width='5'></td><td><a href='" & url & totalPage& "'><img src='/images/btn_nn.gif' width='15' height='11'></a></td>"
'response.write "<td width='5'></td><td><img src='/images/btn_nn.gif' width='15' height='11'></td>"
