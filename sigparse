function ImgSize(w,h,n){
var max_h = 2000;
var max_w = 540;
var height = h;
var width = w;
var rate_h = height/max_h;
var rate_w = width/max_w;
var resize_h = 0;
var resize_w = 0;

if(width > max_w && height > max_h){
if (width > height){
resize_h = height/rate_w;
resize_w = max_w;
if(resize_h > max_h){
rate_h = resize_h/max_h;

resize_h = resize_h/rate_h;
}
}else{
resize_h = max_h;
resize_w = width/rate_h;
if(resize_w > max_w){
rate_w = resize_w/max_w;

resize_w = resize_w/rate_w;
}
}
}else{
if(width <= max_w && height > max_h){
resize_h = height/rate_h;
resize_w = width/rate_h;
}else if(width > max_w && height <= max_h){
resize_h = height/rate_w;
resize_w = width/rate_w;
}else{
resize_h = height;
resize_w = width;
}

}
var im = eval("document.upimage"+n);
im.height = resize_h;
im.width = resize_w;
}
