'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
' 남은 티켓이 얼마나 되나 알아보기 위한 것...
sql = "SELECT count(num) FROM SIGGRAPH_RGST where  REGI<>'Group' and ElectronicTheatre like '%13 Dec 2011: 19:00~21:00%' and paygubun<>'C' and paygubun='Y'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'Group' and ElectronicTheatre like '%14 Dec 2011: 19:00~21:00%' and paygubun<>'C' and paygubun='Y'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'Group' and ElectronicTheatre like '%15 Dec 2011: 16:15~18:15%' and paygubun<>'C' and paygubun='Y'"  '첫째날..
sql = "SELECT count(num) FROM SIGGRAPH_RGST where REGI<>'Group' and ElectronicTheatre like '%15 Dec 2011: 19:00~21:00%' and paygubun<>'C' and paygubun='Y'"  '첫째날..
'##################### 추가 티켓 좌석이 남아있는지 확인 ####################3
sql = "SELECT sum(Convert(int,AddElectronicTheatre15)),sum(Convert(int,AddElectronicTheatre16)),sum(Convert(int,AddElectronicTheatre17)),sum(Convert(int,AddElectronicTheatre18)) FROM SIGGRAPH_RGST where REGI<>'Group' and paygubun<>'C' and paygubun='Y'"
Elect15_rev    = (trim(info(0))+ Con1DaySoo)  ' 전체 개수 - 첫째날 여분 수..
sqlstr = "select count(*) from "&tb&"  where REGI<>'Group'"&event_query2
'      sqlstr = "select COUNT(a.GROUPCODE) from GROUPRegistCode A join SIGGRAPH_RGST B ON A.GROUPCODE = B.GROUPCODE OR A.GROUPCODE = B.GROUPSTUCODE "
'      set info = Server.CreateObject("ADODB.RecordSet")
'      info.open sqlstr,objConn,0,1
'    totalcnt2 =242 - info(0)
'    info.close
'    set info = nothing
