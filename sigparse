../siggraph/AsiaReg/config/functions_db.asp
'############ 카테고리 이름 추출 ###########
'############ 게시판 이름 추출 ###########
'############ 게시판 뎁스 추출 ###########
'############ 게시판 주소 추출 ###########
'########### 게시판 번호, 이름 추출 ##########
'########### 게시판 번호, 이름 추출 ##########
"   WHERE BD_GRP = ? and BD_STATE = '1'  "&_
'################################## 게시판 메인1 ############################################
'################################## 게시판 메인1 ############################################
'################################## 게시판 메인2 ############################################
'################################## 게시판 메인2 ############################################
'################################## 카테고리 리스트 ############################################
g_str = " AND (C_NO = '1' OR C_NO = '2' ) "
g_str = " AND (C_NO = '3' OR C_NO = '4' OR C_NO = '5' ) "
g_str = " AND (C_NO = '6' OR C_NO = '7' OR C_NO = '8' OR C_NO = '9' OR C_NO = '10' OR C_NO = '11' ) "
'################################## 카테고리 리스트 ############################################
'################################## 운영클럽 카테고리 리스트 ############################################
'################################## 운영클럽 카테고리 리스트 ############################################
