'Select Case conference
' Case "Full_Conference"
'
' '컨트리뷰터는 무조건 회원가..
' ' if MemGBN<>"StuMEMBER" then MemGBN="SIGMEMBER"
' 'if MemGBN="SIGMEMBER" or MemGBN="EuroMEMBER" then ' 회원인 경우
' ' if sepdate <=Before1Nov then
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_MEMBER_EARLY"))
' ' Early_gubun = "Before " & resources.Item("EARLY_BIRD_CUTOFF_PLUS_1")
' ' elseif sepdate > Before1Nov and sepdate<=After31Oct then
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_MEMBER_REG"))
' ' Early_gubun = "After " & resources.Item("EARLY_BIRD_CUTOFF")
' ' else
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_MEMBER_ONSITE"))
' ' Early_gubun = "After " & resources.Item("ONLINE_REG_CLOSE_DAY_SHORT")
' ' end if
' 'elseif MemGBN="StuMEMBER" then ' 학생인경우
' ' if sepdate <=Before1Nov then
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_STUDENT_EARLY"))
' ' Early_gubun = "Before " & resources.Item("EARLY_BIRD_CUTOFF_PLUS_1")
' ' elseif sepdate > Before1Nov and sepdate<=After31Oct then
' ' ConferencePay =CINT(resources.Item("PASS_PRICE_STUDENT_REG "))
' ' Early_gubun = "After " & resources.Item("EARLY_BIRD_CUTOFF")
' ' else
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_STUDENT_ONSITE"))
' ' Early_gubun = "After " & resources.Item("ONLINE_REG_CLOSE_DAY_SHORT")
' ' end if
' 'else ' 그외..
' ' if sepdate <=Before1Nov then
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_NONMEM_EARLY"))
' ' Early_gubun = "Before " & resources.Item("EARLY_BIRD_CUTOFF_PLUS_1")
' ' elseif sepdate > Before1Nov and sepdate<=After31Oct then
' ' ConferencePay = CINT(resources.Item("PASS_PRICE_NONMEM_REG"))
' ' Early_gubun = "After " & resources.Item("EARLY_BIRD_CUTOFF")
' ' else
' ' ConferencePay =CINT(resources.Item("PASS_PRICE_NONMEM_ONSITE"))
' ' Early_gubun = "After " & resources.Item("ONLINE_REG_CLOSE_DAY_SHORT")
' ' end if
' 'end if
'
'
'
' Case "Basic"
' if MemGBN="SIGMEMBER" or MemGBN="EuroMEMBER" or MemGBN="StuMEMBER" then ' 회원인 경우
' if sepdate <=Before1Nov then
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_EARLY"))
' Early_gubun = "Before " & resources.Item("EARLY_BIRD_CUTOFF_PLUS_1")
' elseif sepdate > Before1Nov and sepdate<=After31Oct then
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_REG"))
' Early_gubun = "After " & resources.Item("EARLY_BIRD_CUTOFF")
' else
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_ONSITE"))
' Early_gubun = "After " & resources.Item("ONLINE_REG_CLOSE_DAY_SHORT")
' end if
' else ' 그외..
' if sepdate <=Before1Nov then
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_EARLY"))
' Early_gubun = "Before " & resources.Item("EARLY_BIRD_CUTOFF_PLUS_1")
' elseif sepdate > Before1Nov and sepdate<=After31Oct then
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_REG"))
' Early_gubun = "After " & resources.Item("EARLY_BIRD_CUTOFF")
' else
' ConferencePay = CINT(resources.Item("BASIC_PASS_PRICE_ONSITE"))
' Early_gubun = "After " & resources.Item("ONLINE_REG_CLOSE_DAY_SHORT")
' end if
' end if
' Case "Computer_Ani"
' ConferencePay = 150
' Case "Exhibits"
' ConferencePay = 60
' End Select
' conference pay for any contributor is always based on member_early price
' nk - 7/31/12
'response.write conferencepay
'response.end
