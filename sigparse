../siggraph/AsiaReg/admin/function/etc.asp
<!--METADATA TYPE="typelib"  NAME="ADODB Type Library UUID= "00000205-0000-0010-8000-00AA006D2EA4" -->
'################################################################################
'# Purpose  : 기타 공통 함수
'# Filename  : etc.asp
'# Author  : ikara
'# Date    : 2006-03-31
'################################################################################
'############################################################################################
'# Purpose    : 조회수 업데이트
'# Arguments  : connection, command, 테이블, 게시물 idx
'# Returns    : 적용 row 수
'# Author    : ikara
'# Date      : 2006-03-29
'# Modify date  : author : memo
'############################################################################################
'############################################################################################
'# Purpose    : 사용여부 업데이트
'# Arguments  : connection, command, 테이블, 게시물 idx, value
'# Returns    : 적용 row 수
'# Author    : ikara
'# Date      : 2006-03-29
'# Modify date  : author : memo
'############################################################################################
query = "UPDATE " & tb & " SET isdisplay='" & value & "' WHERE idx=" & idx
'###################################################
'# Purpose    : 코드테이블 참조 코드값에 따른 코드명 리턴
'# Arguments  : 카테고리, 코드값
'# Returns    : String
'# Author    : 박정화(jwpark@pulipinc.com)
'# Date      : 2006-06-01
'# Modify date  : author : memo
'###################################################
query = "SELECT name, code FROM tcode WHERE isdel='N' AND category='" & category & "'  AND code = '" & code & "' ORDER BY displayorder ASC"
'###################################################
'# Purpose    : 코드테이블 참조 코드값에 따른 코드명 리턴
'# Arguments  : 카테고리
'# Returns    : String
'# Author    : 박정화(jwpark@pulipinc.com)
'# Date      : 2006-06-01
'# Modify date  : author : memo
'###################################################
query = "SELECT code, name  FROM tcode WHERE isdel='N' AND category='" & category & "' ORDER BY displayorder ASC"
'###################################################
'# Purpose    : 현재 사용중인 폰트 리스트 리턴
'# Arguments  :
'# Returns    : String
'# Author    : 박정화(jwpark@pulipinc.com)
'# Date      : 2006-06-02
'# Modify date  : author : memo
'###################################################
query = "SELECT idx, name  FROM tfont WHERE isdel='N'  ORDER BY idx ASC"
'###################################################
'# Purpose    : 현재 사용중인 관리자 리스트
'# Arguments  :
'# Returns    : String
'# Author    : 박정화(jwpark@pulipinc.com)
'# Date      : 2006-06-08
'# Modify date  : author : memo
'###################################################
query = "SELECT idx, userid, name, place, position, email, grade FROM tadmin WHERE isdel='N'  ORDER BY name ASC"

../siggraph/AsiaReg/admin/function/fileupload.asp
'################################################################################
'# Purpose  : 파일 업로드 유틸모음
'# Filename  : fileupload.asp
'# Author  : ikara
'# Date    : 2006-03-25
'################################################################################
'################################################################################
'# Purpose    : 업로드 파일 확장자 체크
'# Arguments  : 경로포함파일명, 확장자체크구분(1:업로드불가, 2:업로드가능), 제한확장자문자열 변수명
'# Returns    : T/F
'# Author    : ikara
'# Date      : 2006-03-25
'# Modify date  : author : memo
'################################################################################
cob_filename=mid(filename,instrrev(filename,"\")+1)'경로 제외 파일명
file_ext=Lcase(mid(cob_filename,instrrev(cob_filename,".")+1))'맨 마지막 .이후 확장자를 얻는다.
If checkType=1 Then  ' ture 업로드 불가 파일 체크
ElseIf checkType=2 Then   'true 업로드 가능 파일 체크
'################################################################################
'# Purpose    : 업로드 파일 사이즈 kbyte로 변경
'# Arguments  : 파일사이즈 (byte)
'# Returns    : 파일사이즈 kbyte
'# Author    : 권윤경
'# Date      : 2006-03-25
'# Modify date  : author : memo
'################################################################################
'################################################################################
'# Purpose    : 파일 중복 체크
'# Arguments  : 파일명(파일경로를 포함) 문자열
'# Returns    : 중복체크된 파일명 (중복될 경우 _숫자포함 파일명변경)
'# Author    : ikara
'# Date      : 2006-03-25
'# Modify date  : author : memo
'################################################################################
file_name=left(filename,instr(filename,".")-1) '파일명을 얻는다
file_ext=mid(filename,instr(filename,".")+1)'확장자를 얻는다.
'################################################################################
'# Purpose    : 업로드파일 파일명 변경 (ex.20020110_12345678.htm)
'# Arguments  : 파일저장경로, 파일명
'# Returns    : 파일명 String
'# Author    : ikara
'# Date      : 2006-03-25
'# Modify date  : author : memo
'################################################################################
'cob_filename=mid(filename,instrrev(filename,"\")+1)'경로 제외 파일명
'cob_filename=uploaday+"_"+cob_filename
file_ext=Lcase(mid(filename,instrrev(filename,".")+1))'맨 마지막 .이후 확장자를 얻는다.
'################################################################################
'# Purpose    : 업로드파일 삭제
'# Arguments  : 파일저장경로, 삭제할파일명
'# Returns    : 파일명 String
