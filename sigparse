function writeCOM(){
aa = document.all.comment.style;
bb = document.f1.arrow;
if(aa){
if(aa.display=="block"){
aa.display="none";
bb.src = "/board/img/a_down.gif";
}else{
aa.display="block";
bb.src = "/board/img/a_up.gif";
}
}
}
