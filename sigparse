'
'
'Public Function Encrypt(Message)
'Dim ed, key
'key = gcKEY
'
'Set ed = CreateObject("CAPICOM.EncryptedData")
'ed.Content = Message
'ed.SetSecret key
'Encrypt = ed.Encrypt
'Set ed = Nothing
'End Function
'
'
'Public Function Decrypt(EncMessage)
'Dim ed, key
'
'key = gcKEY
'Set ed = CreateObject("CAPICOM.EncryptedData")
'
'ed.SetSecret key
'ed.Decrypt EncMessage
'Decrypt = ed.Content
'
'Set ed = Nothing
'End Function
'
'StringVal = request("string")
'
'response.write encrypt("abcd")&"<br>"
'response.write Decrypt(StringVal )&"<br>" 'Decrypt("768582788793728788142845142731798774867814284514273186877486781428451427318587748678142845142731747777911428451427317682939814284514273192937493781428451427319982891428451427317688948793919814284514273189818887781428451427317974971428451427317886748285142845142731768574929214284587888772867591")
