../siggraph/AsiaReg/admin/function/dbfunction.asp
<!--METADATA TYPE="typelib"  NAME="ADODB Type Library UUID= "00000205-0000-0010-8000-00AA006D2EA4" -->
'################################################################################
'# Purpose  : 데이타 입력/수정/삭제 관련 실행 함수 모음
'# Filename  : dbfunction.asp
'# Author  : ikara
'# Date    : 2006-03-31
'################################################################################
'##########################################################
'# Purpose    : data insert
'# Arguments  : Connection,command, table, Dictionary
'# Returns    : 실행결과 (1:성공, 0:실패)
'# Author    : ikara
'# Date      : 2006-03-27
'# Modify date  : author : memo
'##########################################################
'On error resume next
sValue =  sValue & "N'" & convertQuery(objdict.Item(sKey)) & "'"
'response.write query
Insert = 1          '//정상
Insert = 0           '//오류
'##########################################################
'# Purpose    : data update
'# Arguments  : Connection,command, table, Dictionary, where 절
'# Returns    : 실행결과 (1:성공, 0:실패)
'# Author    : ikara
'# Date      : 2006-03-27
'# Modify date  : author : memo
'##########################################################
'On ERROR resume next
setquery =  setquery & Cstr(sKey)  & " = '" & convertQuery(objdict.Item(sKey)) & "' "
'response.write query
Update = 1          '//정상
Update = 0           '//오류
'##########################################################
'# Purpose    : data delete
'# Arguments  : Connection,command, table, idx
'# Returns    : 실행결과 (1:성공, 0:실패)
'# Author    : ikara
'# Date      : 2006-03-27
'# Modify date  : author : memo
'##########################################################
'On error resume next
'response.write query
Delete = 1          '//정상
Delete = 0           '//오류
'##########################################################
'# Purpose    : data delete  - isdel update
'# Arguments  : Connection,command,  table, 게시물 idx
'# Returns    : 실행결과 (1:성공, 0:실패)
'# Author    : ikara
'# Date      : 2006-03-27
'# Modify date  : author : memo
'##########################################################
'On error resume next
query = "UPDATE " & tb & " SET isuse='N', lastuser=" & lastuser & ", lastip='" & lastip & "', lastdatetime=getdate() WHERE idx=" & idx
DeleteUpdate = 1          '//정상
DeleteUpdate = 0           '//오류
'############################################################################################
'# Purpose    : 사용여부 업데이트
'# Arguments  : Connection,command, table, 게시물 idx
'# Returns    : 아랫글
'# Author    : ikara
'# Date      : 2006-03-29
'# Modify date  : author : memo
'############################################################################################
'On error resume next
query = "UPDATE " & tb & " SET isuse='" & val & "', lastuser=" & lastuser & ", lastip='" & lastip & "', lastdatetime=getdate() WHERE idx=" & idx
IsuseUpdate = 1          '//정상
IsuseUpdate = 0         '//오류
'############################################################################################
'# Purpose    : 데이타 insert, update 쿼리 실행 함수
'# Arguments  : Connection,command, query
'# Returns    : 실행결과
'# Author    : ikara
'# Date      : 2006-03-29
'# Modify date  : author : memo
'############################################################################################
'On error resume next
'response.write query
ExecQuery = 1          '//정상
ExecQuery = 0         '//오류
'############################################################################################
'# Purpose    : rowCount값을 가져온다.
'# Arguments  : Connection,recordset, query
'# Returns    : rowcount
'# Author    : ikara
'# Date      : 2006-05-16
'# Modify date  : author : memo
'############################################################################################
'On error resume next
'##########################################################
'# Purpose    : 게시물 전체갯수
'# Arguments  : totalCount, 한페이지 게시물 수
'# Returns    : totalPage
'##########################################################
'##########################################################
'# Purpose    : 게시물 전체갯수
'# Arguments  : totalCount, 한페이지 게시물 수
'# Returns    : totalPage
'##########################################################
'totalCount  = Cint(objRs(0))
'Response.write totalCount & " <br>"
'##########################################################
'# Purpose    : 게시판 목록
'# Arguments  : objConnection,recordset, table, request, 한페이지 게시물 수, 현재페이지, totalCount, totalPage
'# Returns    : 게시판 목록 정보를 담은 배열
'##########################################################
'##########################################################
'# Purpose    : 게시판 목록
'# Arguments  : objConnection,recordset, table, request, 한페이지 게시물 수, 현재페이지, totalCount, totalPage
'# Returns    : 게시판 목록 정보를 담은 배열
'##########################################################
'##########################################################
'# Purpose    : 게시판 상세정보
'# Arguments  : objConnection,recordset, table, request, 한페이지 게시물 수, 현재페이지, totalCount, totalPage
'# Returns    : 게시판 상세 정보를 담은 배열
'##########################################################
'###################################################
'# Purpose    : 코드테이블 참조 코드값 select option 생성
'# Arguments  : 카테고리,DB저장값, page type (write,modify)
'# Returns    : Html String
'# Author    : 김미의(ikara@pulipinc.com)
'# Date      : 2006-05-11
'# Modify date  : author : memo
'###################################################
query = "select " & column & " from " & tb & " where isuse='Y' " & where
html = html & "<option value='" & arrRs(1,i)  & "'"
'##########################################################
'# Purpose    : 코드테이블 Value
'# Arguments  : objConnection,recordset,category-속성구분, form name, DB값-배열, page type (write,edit)
'# Returns    : 코드 value값 리턴
'# Author    : 김미의(ikara@pulipinc.com)
'# Date      : 2006-06-12
'# Modify date  : author : memo
'##########################################################
query = "SELECT " & column & " FROM " & tb & " WHERE isuse='Y' " & where
'##########################################################
'# Purpose    : 우편번호 찾기
'# Arguments  : objConnection,recordset, table, request
'# Returns    : totalcount
'# Author      : 김종갑(jkkim@pulipinc.com)
'# Date        : 2006-07-04
'# Modify date  : author : memo
'##########################################################
'//변수선언
'//값세팅
where = " WHERE dong like '%"+dong+"%'"
'//ID 중복 체크를 한다
query ="SELECT zipcode+' '+sido+' '+gugun+' '+dong+' '+isnull(bunji,'') as address, zipcode , sido+' '+gugun+' '+dong as address1 FROM " & tb & where
'response.write query

