<!--#include virtual="/admin/function/error.asp"-->
<!--#include virtual="/config/functions.asp"-->
'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
PAYQUERY = " and PAYGUBUN='"&payment&"' "
if search<>""  then search_query =" and "&searchType&" like '%"&search&"%'"
sqlstr = "select count(*) from "&tb&" where convert(smalldatetime,left(app_date,11))='"&code&"'"&event_query &search_query & PAYQUERY
sqlstr = "select count(*) as cnt , sum(convert(money,TotalPay)) as C_sum from "&tb&" where (PAYGUBUN='Y')"
curpage = cint(Request("GoTopage"))    'cint 는 정수로..
<!--<td align="center" bgcolor="#cccccc" width="5%"> Del </td>    -->
sql= "SELECT count(num) as cnt FROM "&tb&" where convert(smalldatetime,left(app_date,11))='"&code&"'" &event_query &search_query & PAYQUERY
DbRec.CursorType = 1    '키 세트 커서. 다른 사용자의 변경 데이터를 표시가능
SQL = SQL & "(SELECT TOP " & ((curpage - 1) * ipp ) & " num FROM "&tb&"  where convert(smalldatetime,left(app_date,11))='"&code&"' "&search_query & PAYQUERY&" "&ALIGNQUERY&")  and convert(smalldatetime,left(app_date,11))='"&code&"' "&search_query & PAYQUERY&" "&ALIGNQUERY&""
totpage = postcount \ ipp  '역슬래시는 정수로 나눗셈..
do until DbRec.EOF or i>DbRec.PageSize   'pageSize 의 기본값은 10
<!--<td align="center"> 삭제 </td>    -->

