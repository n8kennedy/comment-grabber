'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
CheckValue = replace(CheckValue, "''" , "'")
CheckValue = replace(CheckValue, "&quot;", "'")
cmd_query = " where convert(smalldatetime,left(app_date,11))='"&code&"'"
cmd_query = " where (PAYGUBUN is null or PAY_GUBUN='N')"
cmd_query = " where (PAYGUBUN='R')"
cmd_query = " where (PAYGUBUN='Y')"
cmd_query = " where (PAYGUBUN='C')"
cmd_query = " where (serial_num is not null and serial_num<>'' )"
elseif cmd ="gen" then ' # Confernece Attendee
cmd_query = " where (REGI='Attend')"
elseif cmd ="grpmem" then ' # Confernece Attendee
cmd_query = " where (REGI='GroupMember')"
elseif cmd ="grp" then ' # Confernece Attendee
cmd_query = " where (REGI='Group')"
elseif cmd ="contri" then ' # Confernece Attendee
cmd_query = " where (REGI='Contributor' and ContributorType<>'Partner-Partner')"
elseif cmd ="contriPart" then ' # Confernece Attendee
cmd_query = " where (REGI='Contributor' and ContributorType='Partner-Partner')"
elseif cmd ="exhi" then ' # Confernece Attendee
cmd_query = " where (REGI='Exhibition')"
elseif cmd ="Exhibitor" then ' # Confernece Attendee
cmd_query = " where (Regi='Contributor' and contributortype ='Exhibitors-Exhibitors')"
elseif cmd ="bank" then ' # Confernece Attendee
cmd_query = " where (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer' or paybanktransgbn='Neodigm Account')"
sqlstr = "SELECT  D.groupcode,C.* FROM SIGGRAPH_RGST C left outer join  GROUPRegistCode D on C.groupcode = D.groupcode or C.groupstucode = D.groupcode where C.regi<>'Group' and C.nametak is null  UNION aLL select * from GROUPRegistCode B where B.groupcode not in (select a.groupstucode from siggraph_rgst A join GROUPRegistCode B on a.groupstucode = b.Groupcode) and B.groupcode not in (select a.groupcode from siggraph_rgst A join GROUPRegistCode B on a.Groupcode = b.Groupcode)  "
'sqlstr = "SELECT  * FROM "&tb & cmd_query&" where nametak is null order by num asc"
