'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
if code = "search" then Code_query =" and "&searchType&" like '%"&search&"%'"
sqlstr = "select count(*) from "&tb&" where (PAYGUBUN='Y' and (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer'))"&Code_query
sqlstr = "select count(*) as cnt , sum(convert(money,TotalPay)) as C_sum from "&tb&" where (PAYGUBUN='Y' and (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer') or paybanktransgbn='Neodigm Account')"
curpage = cint(Request("GoTopage"))    'cint 는 정수로..
sql= "SELECT count(num) as cnt FROM "&tb&" where (PAYGUBUN='Y' and (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer') or paybanktransgbn='Neodigm Account')"&Code_query
DbRec.CursorType = 1    '키 세트 커서. 다른 사용자의 변경 데이터를 표시가능
SQL = SQL & "(SELECT TOP " & ((curpage - 1) * ipp ) & " num FROM "&tb&" where (PAYGUBUN='Y' and (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer') or paybanktransgbn='Neodigm Account') "&Code_query&" "&ALIGNQUERY&" ) and (PAYGUBUN='Y' and (paybanktransgbn='Bank Tranfer(KOR)' or paybanktransgbn='Intl Bank Transfer') or paybanktransgbn='Neodigm Account') "&Code_query&" "&ALIGNQUERY&""
'response.write SQL
totpage = postcount \ ipp  '역슬래시는 정수로 나눗셈..
do until DbRec.EOF or i>DbRec.PageSize   'pageSize 의 기본값은 10

