'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
if code = "search" then Code_query =" and "&searchType&" like '%"&search&"%'"
sqlstr = "select count(*) from "&tb&" where (REGI='GroupMember')"&Code_query
sqlstr = "select count(*) as cnt , sum(convert(money,TotalPay)) as C_sum from "&tb&" where PAYGUBUN='C'"
curpage = cint(Request("GoTopage"))    'cint 는 정수로..
sql= "SELECT count(num) as cnt FROM "&tb&" where (REGI='GroupMember')"&Code_query
DbRec.CursorType = 1    '키 세트 커서. 다른 사용자의 변경 데이터를 표시가능
SQL = SQL & "(SELECT TOP " & ((curpage - 1) * ipp ) & " num FROM "&tb&" where PAYGUBUN='C' "&Code_query&" order by num desc ) and PAYGUBUN='C'  "&Code_query&" order by num desc"
'response.write SQL
totpage = postcount \ ipp  '역슬래시는 정수로 나눗셈..
do until DbRec.EOF or i>DbRec.PageSize   'pageSize 의 기본값은 10
