'##########################################################
'# 변수 선언
'##########################################################
'//페이지 기본 세팅 정보
if code = "search" then Code_query =" and "&searchType&" like '%"&search&"%'"
sqlstr = "select count(*) from PhotoshopWorld2009 where (PAY_GUBUN is null or PAY_GUBUN='N')"&Code_query
sqlstr = "select count(*) as cnt , sum(convert(money,Pay_sum)) as C_sum from PhotoshopWorld2009 where (PAY_GUBUN='Y') and convert(smalldatetime,left(pay_date,10)) < convert(smalldatetime,left(getdate(),10))"
sqlstr = "select count(*) as cnt , sum(convert(money,Pay_sum)) as C_sum from PhotoshopWorld2009 where (PAY_GUBUN='Y')"
sqlstr = "select count(*) as cnt , sum(convert(money,Pay_sum)) as C_sum from PhotoshopWorld2009 where (PAY_GUBUN='C')"
curpage = cint(Request("GoTopage"))    'cint 는 정수로..
sql= "SELECT count(log_idx) as cnt FROM PhotoshopWorld2009 where PAY_GUBUN is null or PAY_GUBUN='N'"&Code_query
DbRec.CursorType = 1    '키 세트 커서. 다른 사용자의 변경 데이터를 표시가능
SQL = SQL & "(SELECT TOP " & ((curpage - 1) * ipp ) & " log_idx FROM PhotoshopWorld2009 where (PAY_GUBUN is null or PAY_GUBUN='N') "&Code_query&" "&ALIGNQUERY&" ) and (PAY_GUBUN is null or PAY_GUBUN='N')  "&Code_query&" "&ALIGNQUERY&""
'response.write SQL
totpage = postcount \ ipp  '역슬래시는 정수로 나눗셈..
do until DbRec.EOF or i>DbRec.PageSize   'pageSize 의 기본값은 10

