Response.Write "<td><img src='"&path&"/num_pp.gif'></td><td width='5'></td>"
Response.Write "<td><a href='" & url & blockPage-listNum & "'><img src='"&path&"/num_p.gif'></a></td><td width='10'></td>"
Response.Write "<td><img src='"&path&"/num_p.gif'></td><td width='10'></td>"
Response.Write "<td  class='num' width=''>"
Response.write "<font class='txt_orange12'><b>" & blockPage & "</b></font>&nbsp;"
'Response.Write "<img src='/images/gray_bar.gif' width='15' height='12' align='absmiddle'>"
Response.Write "<td width='10'></td><td><a href='" & url &  blockPage  & "'><img src='"&path&"/num_n.gif' ></a></td>"
